<Application x:Class="Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vms="clr-namespace:ViewModels;assembly=ViewModels"
             xmlns:vmi="clr-namespace:ViewModels.Internal;assembly=ViewModels"
             xmlns:vme="clr-namespace:ViewModels.External;assembly=ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <vms:MainViewModel x:Key="MainViewModel" />

        <vme:CompanyTableViewModel x:Key="CompanyTableViewModel" />
        <vme:CompanyViewModel x:Key="CompanyViewModel" />

        <vmi:FileViewModel x:Key="FileViewModel" />





        <SolidColorBrush x:Key="Clear"
                         Opacity="0"/>
        
        <SolidColorBrush x:Key="b"
                         Opacity="0.25"
                         Color="Black" />
        <SolidColorBrush x:Key="bb"
                         Opacity="0.5"
                         Color="Black" />
        <SolidColorBrush x:Key="bbb"
                         Opacity="0.75"
                         Color="Black" />
        <SolidColorBrush x:Key="bbbb"
                         Opacity="1"
                         Color="Black" />

        <SolidColorBrush x:Key="f"
                         Opacity="0.25"
                         Color="White" />
        <SolidColorBrush x:Key="ff"
                         Opacity="0.5"
                         Color="White" />
        <SolidColorBrush x:Key="fff"
                         Opacity="0.75"
                         Color="White" />
        <SolidColorBrush x:Key="ffff"
                         Opacity="1"
                         Color="White" />
    </Application.Resources>
</Application>
