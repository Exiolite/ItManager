             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vi="clr-namespace:ViewModels.Internal;assembly=ViewModels"
             xmlns:ve="clr-namespace:ViewModels.External;assembly=ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ve:DataContextViewModel x:Key="ExternalDataContextViewModel" />
        <vi:DataContextViewModel x:Key="InternalDataContextViewModel" />
    </Application.Resources>
</Application>
