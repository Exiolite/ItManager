<UserControl DataContext="{StaticResource ADUserViewModel}"
             x:Class="View.External.ADUserView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:View.External"
             mc:Ignorable="d">
    <StackPanel Style="{StaticResource SPMargin}">
        <TextBox Text="{Binding PropADUser.PropUserName}" />
        <TextBox Text="{Binding PropADUser.PropPassword}" />
    </StackPanel>
</UserControl>
