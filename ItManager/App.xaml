<Application x:Class="ItManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:ItManager.ViewModel"
             StartupUri="View/MainWindowView.xaml">
    <Application.Resources>
        <vm:MainViewModel x:Key="MainViewModel" />
        <vm:DomainViewModel x:Key="DomainViewModel" />
        <vm:DevicesViewModel x:Key="DevicesViewModel" />
        <vm:ServersListViewModel x:Key="ServersListViewModel" />
        <vm:ServerViewModel x:Key="ServerViewModel" />
        <vm:ComputersListViewModel x:Key="ComputersListViewModel" />
        <vm:ComputerViewModel x:Key="ComputerViewModel" />
        <vm:TasksListViewModel x:Key="TasksListViewModel" />
        <vm:TaskViewModel x:Key="TaskViewModel" />
    </Application.Resources>
</Application>
