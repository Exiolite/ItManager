<Application x:Class="ItManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:ItManager.ViewModel"
             xmlns:m="clr-namespace:ItManager.Model"
             xmlns:v="clr-namespace:ItManager.View"
             StartupUri="View/MainWindowView.xaml">
    <Application.Resources>
        <vm:MainViewModel x:Key="MainViewModel"/>
        <vm:DomainViewModel x:Key="CompanyViewModel"/>
        <vm:ComputerViewModel x:Key="ComputerViewModel"/>
        <v:ComputerWinowView x:Key="ComputerWinowView"/>
        <v:AnyDeskWindowView x:Key="AnyDeskWindowView"/>
        <m:AnyDesk x:Key="AnyDesk"/>
    </Application.Resources>
</Application>
